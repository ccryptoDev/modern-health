            <li><a data-toggle="tab" href="#commentsection" >Comments</a></li>
            <li><a data-toggle="tab" href="#loagactivitysection" >Log</a></li>
          </ul>

          <div class="tab-content">
            <div id="userinfo" class="tab-pane fade in active">
              <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <h3>User Information
                 <div class="pull-right text-left col-lg-7 col-md-7 col-sm-12 col-xs-12">
                    <!-- Approve button -->
                      {% if req.session.rolename =='Underwriter' or req.session.rolename =='Admin'  or  req.user.rolename =='PracticeAdmin' or req.user.rolename =='PracticeStaff' %}
                           {% if paymentmanagementdata.achstatus==0 %}
                            <button type="button" class="btn btn-primary" id="approveloan"  onclick="setApproveLoan();">Approve</button>&nbsp;&nbsp;
                           {% endif %}
                       {% endif %}

                      {% if req.session.rolename =='Underwriter' or req.session.rolename =='Admin'  or  req.user.rolename =='PracticeAdmin' or req.user.rolename =='PracticeStaff' %}
                           {% if paymentmanagementdata.achstatus==1 and paymentdebitCount == 0 and paymentmanagementdata.procedureWasConfirmed != 1 %}
                            <button type="button" class="btn btn-primary" id="btnUpdateProcedureDate"  onclick="updateApproveLoan();">Update procedure date</button>&nbsp;&nbsp;
                          {% endif %}
                       {% endif %}

                        {% if req.session.rolename =='Underwriter' or req.session.rolename =='Admin'  or  req.user.rolename =='PracticeAdmin' or req.user.rolename =='PracticeStaff' %}
                            {% if paymentmanagementdata.moveToArchive == 1 %}
                              <div style="position: absolute; left: 250px; top: 0px;">
                                <button type="button" class="btn btn-primary" id="unarchive"  onclick="$('#movetoUnarchiveModal').modal('show');">Unarchive</button>
                              </div>
                            {% else %}
                              <div style="position: absolute; left: 250px; top: 0px;">
                                <button type="button" class="btn btn-primary" id="archive"  onclick="$('#movetoarchiveModal').modal('show');">Archive</button>
                              </div>
                            {% endif %}
                        {% endif %}


                       <!--<button type="button" class="btn btn-primary" id="approveloan" data-toggle="modal" data-target="#approveModal">Approve</button> &nbsp;&nbsp;-->
                      <!-- Release button -->
                              {% if req.session.rolename =='Underwriter' or req.session.rolename =='Admin'  or  req.user.rolename =='PracticeAdmin' or req.user.rolename =='PracticeStaff' %}
                                   {% if paymentmanagementdata.achstatus==3 %}
                                      <button type="button" class="btn btn-primary" id="releaseloan" data-toggle="modal" data-target="#releaseModal">Release</button>&nbsp;&nbsp;
                                   {% endif %}
                              {% endif %}
                      <!-- Deny button -->
                      {% if req.session.rolename =='Underwriter' or req.session.rolename =='Admin'  or  req.user.rolename =='PracticeAdmin' or req.user.rolename =='PracticeStaff' %}
                        {% if paymentmanagementdata.achstatus==0 or paymentmanagementdata.achstatus==3 %}
                                  <button type="button" class="btn btn-primary" id="denyloan" data-toggle="modal" data-target="#denyModal">Deny</button>&nbsp;&nbsp;
                        {% endif %}
                      {% endif %}


                     {% if req.session.rolename =='Admin'  or  req.user.rolename =='PracticeAdmin' %}
                         {% if paymentmanagementdata.achstatus==1 and (paymentmanagementdata.status =='OPENED' or paymentmanagementdata.status == "Current") and paymentmanagementdata.procedureWasConfirmed != 1 and showApproveButton %}
                             &nbsp;
                             <button type="button" class="btn btn-primary" id="btnInitiateConfirmProcedure"  onclick="initiateConfirmProcedure();">Confirm Procedure</button>
                         {% endif %}
                     {% endif %}

                       {% if req.session.rolename =='Underwriter' or req.session.rolename =='Admin'  or  req.user.rolename =='PracticeAdmin' or req.user.rolename =='PracticeStaff' %}
                        {% if paymentDatediff > 45 and paymentmanagementdata.moveToOpen != 1 and ( paymentmanagementdata.achstatus == 0 or paymentmanagementdata.achstatus == 2 ) %}
                              <!--<button type="button" class="btn btn-primary" id="movetoopen" onclick="moveToOpenApplication();">Move to open</button> -->
                        {% endif %}
                      {% endif %}

                     {% if req.session.rolename =='Underwriter' or req.session.rolename =='Admin'  or  req.user.rolename =='PracticeAdmin' or req.user.rolename =='PracticeStaff' %}
                    {% if paymentmanagementdata.appverified == 0 and paymentmanagementdata.achstatus == 2 %}
                               <!-- <button type="button" class="btn btn-primary" id="moveToreview" onclick="markAsReviewApp();">Mark as reviewed</button> -->
                     {% endif %}
                   {% endif %}




                      {% if req.session.rolename =='Underwriter' or req.session.rolename =='Admin'  or  req.user.rolename =='PracticeAdmin' or req.user.rolename =='PracticeStaff' or req.user.rolename =='PracticeDoctor' %}
                          {% if paymentmanagementdata.achstatus==0 or paymentmanagementdata.achstatus==1%}
                            <div style="position: absolute; right: 0;  top: 0px;">
                              <button type="button" class="btn btn-primary" id="linkstaff"  onclick="doctorsList();">Link staff /
                              doctor</button>&nbsp;&nbsp;
                            </div>
                        {% endif %}
                      {% endif %}

                   </div>
                </h3>
                <br/>

                <div id="responsemessage"></div>

                <div class="table-responsive">
                  <table class="table table-bordered">
                      <tbody>
                        <tr>
                        <th>User Reference </th>
                        <td>
                           <a href="/admin/viewUserDetails/{{user.id}}">
