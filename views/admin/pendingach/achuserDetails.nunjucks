          {% endif %}-->

      {% if paymentmanagementdata.achstatus == 0 %}
          <a href="{{backviewType}}"><button type="button" class="btn btn-primary"><i class="fa fa-chevron-circle-left" aria-hidden="true"></i>&nbsp;&nbsp;Back</button></a>
      {% endif%}

