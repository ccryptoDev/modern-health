                              <p class="text-danger"><i class="fa fa-bell" aria-hidden="true"></i>Email unverified</p>
                            {% endif%}
                        </td>
                      </tr>
                      <tr>
                        <th>Phone Number</th>
                        <td>{{ user.phoneNumber }}</td>
                        <td>
                            {% if user.isPhoneVerified %}
                              <p class="text-success"><i class="fa fa-bell" aria-hidden="true"></i> Phone verified</p>
                            {% else %}
                              <p class="text-danger"><i class="fa fa-bell" aria-hidden="true"></i>Phone unverified</p>
                            {% endif%}
                        </td>
                      </tr>
                      <tr>
                        <th>Date of Birth</th>
                        <td>{{ user.dateofBirth }}</td>
                        <td>&nbsp;</td>
                      </tr>
                              <tr>
                                <th>Latitude</th>
                                <td>
                                  {% if user.latitude %}
                                    {{ user.latitude }}
                                   {% else %}
                                    --
                                   {% endif%}
                                </td>
                                <td>&nbsp;</td>
                              </tr>
                              <tr>
                                <th>Longitude</th>
                                 <td>
                                  {% if user.longitude %}
                                    {{ user.longitude }}
                                   {% else %}
                                    --
                                   {% endif%}
                                </td>
                                <td>&nbsp;</td>
                              </tr>
                      <tr>
                        <th>Address</th>
                        <td>
                            {% if user.street %}
                            {{ user.street }}( <i class="fa fa-edit" style="color:green;" aria-hidden="true"></i>
                              <a href="javascript:void(0);" onclick="editAddressDetails('street','{{ user.street }}')">Edit Street</a> )
                            {% endif%}
                             <br/>
                            {{ user.city }}
                            &nbsp;&nbsp;( <i class="fa fa-edit" style="color:green;" aria-hidden="true"></i>
                            <a href="javascript:void(0);" onclick="editAddressDetails('city','{{ user.city }}');">Edit City</a> )
                            <br/>
                            {{ user.zipCode }}
                            &nbsp;&nbsp;( <i class="fa fa-edit" style="color:green;" aria-hidden="true"></i>
                            <a href="javascript:void(0);" onclick="editAddressDetails('zipcode','{{ user.zipCode }}');">Edit Zipcode</a> )

                            <div style="display:none; padding:10px 0px;" id="changeaddressdiv" class="col-lg-12 col-md-12 col-sm-12 col-xs-12">

                               <form name="changeaddressform" id="changeaddressform" method="post" action="" style="padding:0px; margin:0px;">
                                   <input class="" type="hidden" id="changetype" name="changetype" value="">
                                   <input class="" type="hidden" id="addressPayID" name="addressPayID" value="{{ paymentmanagementdata.id }}">
                                   <input class="" type="hidden" id="addressUserID" name="addressUserID" value="{{ user.id }}">
                                   <label id="addresslabel"></label><br/>
                                   <input type="text" name="addressdata" id="addressdata" value="" style="width:60%; padding:3px;" required>
                                   &nbsp;&nbsp;
                                   <button type="submit" name="changeaddress" id="changeaddress" class="btn btn-primary" style="margin-top:-5px;">Submit</button>
                                    <button type="button" name="canceladdress" id="canceladdress" class="btn btn-primary" style="margin-top:-5px;" onclick="return canceladdressbtn();">Cancel</button>
                                   <br/>
                                   <span id="addressrror"></span>
                                   <span id="addressmessage"></span>
                               </form>

                            </div>

                        </td>
                        <td>&nbsp;</td>
                      </tr>
                      <tr>
                        <th>State</th>
                          <td>
                            {% if user.state %}
                            {{ user.state}}
                            {% else %}
                              --
                            {% endif %}
                        </td>

                        <td>&nbsp;</td>
                      </tr>


                      <tr>
                        <th>Financing Exist</th>
                        <td>
                          {% if paymentmanagementdata.achstatus==1 %}
                          Yes
                          {% else %}
                          No
                          {% endif %}

                        </td>
                        <td>
                            {% if paymentmanagementdata.achstatus==1 %}
                              <p class="text-success"><i class="fa fa-bell" aria-hidden="true"></i> Financing Exists</p>
                            {% else %}
                              <p class="text-danger"><i class="fa fa-bell" aria-hidden="true"></i>No Financing</p>
                            {% endif%}
                        </td>
                      </tr>
                      <tr>
                        <th>Bank Added</th>
                        <td>
                            {% for accountdata in  accountDetail %}
                              {% if accountdata.id== paymentmanagementdata.account %}
                                {{ accountdata.accountName }} - {{ accountdata.accountNumber }}
                              {% endif %}
                            {% endfor %}
                        </td>
                        <td>
                            {% if user.isBankAdded %}
                              <p class="text-success"><i class="fa fa-bell" aria-hidden="true"></i> Bank Added </p>
                            {% else %}
                               <p class="text-success"><i class="fa fa-bell" aria-hidden="true"></i> Bank not yet added</p>
                            {% endif%}
                        </td>
                      </tr>

                      <tr>
                        <th>Registered Date</th>
                        <td>{{ user.createdAt }}
                        </td>
                        <td>&nbsp;</td>
                      </tr>
                      <tr>
                        <th>Last Profile Update Date</th>
                        <td>{{ user.updatedAt }}
                        </td>
                        <td>&nbsp;</td>
                      </tr>

                                <tr>
                        <th>Annual Income </th>
                        <td>{{ annualIncome }}</td>
                                    <td>&nbsp;</td>
                      </tr>
